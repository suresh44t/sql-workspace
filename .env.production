SQL_WORKSPACE_API_URL=https://api.sqlplayground.com/api 
SQL_WORKSPACE_ENVIRONMENT=production 
SQL_WORKSPACE_DEBUG=false 

# Performance Config 
SQL_WORKSPACE_VIRTUALIZATION_ROW_HEIGHT=40 
SQL_WORKSPACE_VIRTUALIZATION_OVERSCAN=5 
SQL_WORKSPACE_MAX_HISTORY_ITEMS=50 

# Feature Flags 
SQL_WORKSPACE_ENABLE_DARK_MODE=true 
SQL_WORKSPACE_ENABLE_QUERY_HISTORY=true 
SQL_WORKSPACE_ENABLE_AUTO_SAVE=true 

# Error Reporting 
SQL_WORKSPACE_ERROR_LOGGING=true 
SQL_WORKSPACE_ERROR_SAMPLING_RATE=0.1 
